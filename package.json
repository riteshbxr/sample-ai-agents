    "demo:cost-tracking": "node src/examples/strategies/cost-tracking/cost-tracking-example.js",
    "demo:batch": "node src/examples/strategies/batch/batch-example.js",
    "demo:memory": "node src/examples/strategies/memory/memory-example.js",
    "demo:prompts": "node src/examples/strategies/prompt-techniques-example.js",
