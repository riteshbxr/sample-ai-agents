{
  "name": "ai-agents-demo",
  "version": "1.0.0",
  "description": "Modern AI agents demo using OpenAI and Claude APIs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/menu.js",
    "menu": "node src/menu.js",
    "demo:interactive-chat": "node src/examples/sdk-usage/interactive-chat.js",
